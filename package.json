{
  "name": "dundas-session-provider",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ts-node": "node -r ./bin/ts-node.js",
    "ts-node:inspect": "node --inspect=9999 --inspect -r ./bin/ts-node.js",
    "ts-node:inspect:brk": "node --inspect=9999 --inspect-brk -r ./bin/ts-node.js",
    "start": "npm run ts-node src/start.ts",
    "start:dev": "npm run ts-node:inspect src/start.ts",
    "start:dev:brk": "npm run ts-node:inspect:brk src/start.ts"
  },
  "main": "lib/dundasSessionProvider.js",
  "dependencies": {
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "request": "2.87.0",
    "requestretry": "2.0.2",
    "sinon": "^6.3.4"
  },
  "devDependencies": {
    "@types/node": "10.9.3",
    "ts-node": "7.0.1",
    "typescript": "3.0.3"
  }
}
